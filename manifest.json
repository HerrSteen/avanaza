{
  "manifest_version": 2,
  "name": "Avanaza",
  "version": "1.0.0",
  "description": "Add buttons to fill in the current price in Avanzas shop-screen.",
  "content_scripts": [
    {
      "js": [
        "fns.js",
        "link.js",
        "button.js",
        "spread.js",
        "sumInput.js",
        "keylistener.js",
        "main.js"
      ],
      "matches": [
        "https://www.avanza.se/handla*"
      ]
    }
  ],
  "icons": {
    "16": "icons/a-16.png",
    "48": "icons/a-48.png",
    "128": "icons/a-128.png"
  }
}
